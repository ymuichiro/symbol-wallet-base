{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.2.0",
    "generators": {
      "main": {
        "generatorName": "typescript-fetch",
        "output": "services/node-client",
        "enablePostProcessFile": true,
        "inputSpec": "specs/node-openapi.yml",
        "globalProperty": {
          "skipFormModel": false
        }
      }
    }
  }
}